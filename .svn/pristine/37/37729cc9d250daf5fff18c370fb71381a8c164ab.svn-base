<script>


$(document).ready(function() {
    $('#select_plan').click(function(){
            var admin_user_count=$('#admin_count').val();
            var general_user_count=$('#general_count').val();
            var limited_user_count=$('#limited_count').val();
            
            if(admin_user_count==0 &&  general_user_count==0 &&  limited_user_count==0 ){
              $('#select_user_error').html('Select Atleast One User')
              return false
            }
            else{
                this.submit();
            }

            
    });	
   
        $('.minus').click(function () {
            var $input = $(this).parent().find('input');
            var count = parseInt($input.val()) - 1;
            count = count < 0 ? 0 : count;
            $input.val(count);
            $input.change();
            return false;
        });
        $('.plus').click(function () {
            var $input = $(this).parent().find('input');
            // $input.val(parseInt($input.val()) + 1);
            var maximum=$('#max_val').val();
            if(maximum==""){
                $input.val(parseInt($input.val()) + 1);
                
            }
            else{
                var count = parseInt($input.val()) + 1;
                count = count > maximum ? maximum : count;
                $input.val(count);
            }
            $input.change();
            return false;
        });


       


        // $('.users_count').keyup(function(){
            $(".users_count").bind("keyup change", function(e) {

                var sess="{{session['currency_symbol']}}";
            var users_count=this.value;
            if(users_count == '')
            {
                users_count=0
            }
            var usertype=$(this).attr("user_type");
            var url='/ajax_load_user_amount';
            $.ajax({
			type:"GET",
			url:url,
			data:{
				 'users_count':users_count,
                 'usertype':usertype
			 },
			success: function (data) {
                new_amount=data.split('@')[0]
                maximum_value=data.split('@')[1]
                $('#max_val').val(maximum_value);
                dat=users_count*new_amount;
                              
                if (usertype==1){
                    $(".admin_user_amount").html(sess+' '+dat)
                  
                    $("#adminus").val(dat)
                    $("#admin_count").attr("max", maximum_value);
                    
                }
                else if (usertype==2){
                    $(".general_user_amount").html(sess+' '+dat)
                    $("#genus").val(dat)
                    $("#general_count").attr("max", maximum_value);
                   
                }
                else if (usertype==3){
                    $(".limited_user_amount").html(' '+sess+' '+dat)
                    $("#limus").val(dat)
                    $("#limited_count").attr("max", maximum_value);
                    
                }

                var adminus = $("#adminus").val();
                 var genus = $("#genus").val();
                 var limus = $("#limus").val();
                var totalamounts=parseFloat(adminus)+parseFloat(genus)+parseFloat(limus);
                    $(".total_amount").html('$ '+totalamounts)
                 $("#totamnt").val(totalamounts)
			      
                  
			}
		 });
        })


        
    });</script>
  </body>
  </html>