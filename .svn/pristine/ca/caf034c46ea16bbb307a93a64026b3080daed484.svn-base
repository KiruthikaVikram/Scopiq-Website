<script>
        
        $('.form_field_input, .form_select').each(function () {
                    if (!$(this).val()) {
                        $(this).parent('.form_field_outer').removeClass('form_field_outer_activated');
                    } else {
                        $(this).parent('.form_field_outer').addClass('form_field_outer_activated');
                        $(this).parent('.form_field_outer').addClass('input_blue_border');
                    }
                });
                $('.form_field_input, .form_select').on('focus', function () {
                    $(this).parent('.form_field_outer').addClass('form_field_outer_activated');
                    $(this).parent('.form_field_outer').addClass('input_blue_border');
                })
                $('.form_field_input, .form_select').on('focusout', function () {
                    $(this).parent('.form_field_outer').removeClass('form_field_outer_activated');
                    $(this).parent('.form_field_outer').removeClass('input_blue_border');
                })
                $('.form_field_input, .form_select').on('click', function () {
                    $(this).parent('.form_field_outer').addClass('form_field_outer_activated');
                })
                $('.form_field_input, .form_select').on('keypress', function () {
                    $(this).parent('.form_field_outer').addClass('form_field_outer_activated');
                })
                $('.form_field_input, .form_select').on('blur', function () {
                    if (!$(this).val()) {
                        $(this).parent('.form_field_outer').removeClass('form_field_outer_activated');
                    } else {
                        $(this).parent('.form_field_outer').addClass('form_field_outer_activated');
                        $(this).parent('.form_field_outer').addClass('input_blue_border');
                    }
                });
                
                </script>

                                    
