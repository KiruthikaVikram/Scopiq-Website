<style>.container {
    width: 100%;
  }
  
  .progressbar {
    width: 105%;
    counter-reset: step;
    margin: 0;
    margin-top: 15px;
    margin-bottom: 15px;
    padding: 0;
  }
  

 
  .progressbar a {
    list-style-type: none;
    float: left;
    width: 160px;
    position: relative;
    text-align: center;
    font-family: sans-serif;
    font-size: 15px;
    font-weight: normal;
    color: #546A79;
    /* Steps*/
  }
  
   



  .progressbar a:before {
    content: counter(step);
    counter-increment: step;
    width: 44px;
    height: 44px;
    line-height: 44px;
    /* border: 4px solid #fff; */
    display: block;
    text-align: center;
    margin: 0 auto 8px auto;
    border-radius: 50%;
    background-color: #E3E3E3;
    /* Circles*/
  }
  
  .progressbar a:after {
    content: '';
    position: absolute;
    width: 73%;
    height: 4px;
    background-color: #E3E3E3;
    /*lines */
    top: 20px;
    left: -37%;
    /* z-index: -1; */
  }

  @media only screen and (max-width: 760px) {
  .progressbar a {
    width: 62px;
    font-size: 12px;
  }

  .progressbar a:after {
    content: '';
    position: absolute;
    width: 42%;
    height: 4px;
    background-color: #E3E3E3;
    top: 20px;
    left: -21%;
    /* z-index: -1; */
}

}


@media only screen and (max-width: 991px) and (min-width: 762px) {
  .progressbar a {
    width: 98px;
    font-size: 12px;
  }

  .progressbar a:after {
    content: '';
    position: absolute;
    width: 59%;
    height: 4px;
    background-color: #E3E3E3;
    top: 20px;
    left: -31%;
    /* z-index: -1; */
}

}



@media only screen and (max-width: 1199px) and (min-width: 992px) {
  .progressbar a {
    width: 133px;
    font-size: 12px;
  }

  .progressbar a:after {
    content: '';
    position: absolute;
    width: 59%;
    height: 4px;
    background-color: #E3E3E3;
    top: 20px;
    left: -31%;
    /* z-index: -1; */
}

}

  
  .progressbar a:first-child:after {
    content: none;
  }
  
  .progressbar a.active {
    color: #546A79;
  }

  .progressbar a.actived:before {
    width: 48px;
    height: 48px;
    line-height: 48px;
    border-radius: 50%;
    border-color: #0073CF;
    color: white;
