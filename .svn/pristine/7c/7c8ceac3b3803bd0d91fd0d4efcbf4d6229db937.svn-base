{% include 'headernew.html' %}
<style>.new_card_box .new_card_features ul li:last-child {
    margin-bottom: 5px;
} </style>
<style>
.new_card_box {  width: 100%; }
.new_card_box .new_card_pricebox {
    float: left;
    width: 100%;
    margin-top: 2px;
    margin-bottom: 0px;
	border-bottom: 1px solid rgba(36, 39, 44, 0.06);
}
.new_card_box .new_card_infobox {
    float: left;
    width: 100%;
    height: 52px;
}
a { color:black}
span { margin-left:20px; }

</style>

<style>
.discount_slider {
    border: none; 
     box-shadow: 0 2px 4px 0 rgba(36, 39, 44, 0.15); 
    margin-bottom: 0px;
     overflow: hidden; 
    width: 228px;
    border-radius: 4px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
}

.discount_percent_section_price {
    display: inline-block;
    font-size: 26px;
    font-family: roboto, sans-serif;
    font-weight: 500;
    margin-top: 14px;
    margin-bottom: 18px;
	color:#472dc2;
}

.discount_percent_section_prices {
    display: inline-block;
    font-size: 20px;
    font-family: roboto, sans-serif;
    font-weight: 500;
    margin-top: 22px;
    margin-bottom: 10px;
}


.discount_section {
    height: 114px;
	padding: 1px;
    text-align: center;
    background-image: linear-gradient(to bottom, #4EC6DE, #4EC6DE);
}

.discount_percent_sections {
    background: #fff;
    margin: 4px;
    border-radius: 4px;
}


#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
  text-align: center;
  font-size: 12px;
}

#customers tr:nth-child(even){background-color: #fff;}

#customers tr:hover {background-color: #fff;}

#customers th {
 
  text-align: center;
  font-size: 12px;
  background-color: #fff;
  color: #000;

}
</style>



<!-- Header Close --> 



<!-- Section About End -->
 

<!--  Section Services End -->
<!-- Section Testimonial Start -->



<section class="section">


	<div class="row justify-content-center">
			<div class="col-lg-12 text-center">
				<div class="section-title">
        <br>
        <h3>Plan Selection</h3>

				 {% include 'steps.html' %}    

				</div>
			</div>
		</div> <br>

        <form class="form-register" id="form_register" action="#" method="post">

 
        <div class="container">



            {% with messages = get_flashed_messages(with_categories=true) %}
            {% if messages %}
                {% for category,message in messages %}
                        <div class="{{category}}">
  
                            {{message}}
  
                        </div>
  
                {% endfor %}
            {% endif %}
        {% endwith %}


            <div class="row">
                <div class="col-lg-12">

								
								<div class="new_card_infobox">
							
								<div class="new_card_info" style="background-image: linear-gradient(to bottom, #139ee8, #0865cc);">
									<p class="card_title" style="margin-bottom: 0;"> Plan Selection </p>
										
                                </div>
					    </div>
                    <div class="cta-item  bg-white p-5 rounded">
                        
                            
                        

                       


                        <div class="row">

<div class="col-lg-2">
</div>
{% if count_values.0.free_trial_expire_count==0 %}      
                <div class="col-lg-4">
                    <div class="swiper-slide new_card_box" style=" height: 357px; margin-right: 20px;">
						<div class="new_card_infobox">

								<div class="new_card_info" style="background-image: linear-gradient(to bottom, #139ee8, #0865cc);">
									<p class="card_title"> 
                                      Free Trail
                                    </p>
                                </div>
					    </div>
						<div class="new_card_pricebox">
                            
                                <p>
                                    
                                 <span style="margin-left: 123px;">
                       3 Months</span>
                                </p>
                        </div>
						<div class="new_card_features" style="overflow-y:scroll">
							    <p>Select Any One Package For Free Trail</p>
								<ul>

                                   

                                    {% for product_lists in product_list %}
                                      
                                          <li>  <input type="radio" name="product_id" class="product_id" value="{{product_lists.product_id}}" {% if product_lists.product_id == 1 %}checked {% endif %}> &nbsp;&nbsp; {{product_lists.product_name}}</li>
                                        
                                    {% endfor %}
            
                                   
            
                                
                                     <!-- <li> &#9679; Document Management</li> -->
                                    <li>  &#9679; 5 General Users</li>
                                    <li>  &#9679; 5 Limited Users</li>
                                    <li>  &#9679; 5 Admin Users</li>
                                </ul>
                        </div>
                                                                                                                                
						<div class="new_card_btns">
                         <button type="submit" name="packages_type" style="width: 71%;" value="freetrial" class="add_to_cart" >Continue with Free Trail</button>	
                    
                        </div>
					</div>
                </div>

                {% endif %}

<div class="col-lg-1">
</div>

                <div class="col-lg-4">
                    <div class="swiper-slide new_card_box" style=" height: 357px; margin-right: 20px;">
						<div class="new_card_infobox">

								<div class="new_card_info" style="background-image: linear-gradient(to bottom, #139ee8, #0865cc);">
									<p class="card_title"> 
                                      Packages
                                    </p>
                                </div>
					    </div>
						<div class="new_card_pricebox">
                            
                                <p>
                                   <span style="margin-left: 123px;">
                       1 Year</span>
                                </p>
                        </div>
						<div class="new_card_features" style="overflow-y:scroll">
							    <p>Includes</p>
								<ul>
                                    <li>  &#9679; Standard  </li> 
                                    <li>  &#9679; Productivity  </li> 
                                    <li>  &#9679; Enterprise  </li> 
                                    <li>  &#9679; Customization </li> 
                                </ul>
                        </div>
                                                                                                                                
						<div class="new_card_btns">
                            <button type="submit" name="packages_type" value="pur_pkg" class="add_to_cart" >Select Packages(payment)</button>	
                    
                        </div>
					</div>
                </div>




                       

 

      
	  	

                        </div> 

            
        


                                
                                  
                                
                        </div>


                      

                    </div>
                </div>
            </div>
        </div>
    </form>
</section>





