<link rel="stylesheet" href="static/plugins1/bootstrap/css/bootstraps.min.css">

<link rel="stylesheet" href="static/css1/style.css"/>

{% include 'headernew.html' %}


<!--  Section Services Start -->


<section class="section service border-top">
	<div class="container">
		<div class="row justify-content-center">
			<div class="col-lg-12 text-center">
				<div class="section-title">
        <br>
        <h3>Cloud Setup</h3>
				 {% include 'steps.html' %}    

				</div>
			</div>
		</div> <br>
     <form class="address-form" id="form_register"method="post" >
		<div class="row justify-content-center">
		

                {% for cloud_lists in cloud_list | sort(attribute='cl_type_id') %}

			<div class="col-lg-3 col-md-6 col-sm-6">
				<div class="swiper-container swiper-container-horizontal">
                                   
					<div class="swiper-wrapper">
            <div class="swiper-slide new_card_box" style="width: 100%; margin-right: 20px;">
              <div class="new_card_infobox">
                <div class="new_card_info{{cloud_lists.cl_type_id}}">
                   <p class="card_title"> {{cloud_lists.cl_type_name}} </p>
                </div>
              </div>
              <div class="new_card_pricebox">
                <p>
                        <p>
                                <span>{{session['currency_symbol']}} {{cloud_lists.amount}}</span>
                               <input type="hidden" name="cloud_amount_{{cloud_lists.cl_type_id}}" value="{{cloud_lists.amount}}">
                                <span>/ Year</span>
                            </p>
                </p>
              </div>
              <div class="new_card_features" style="overflow-y:scroll">
                <p>Includes</p>
                <ul>
                    
                        <li> {{cloud_lists.features| safe}}</li> 
                         

              

                            </ul>
            </div>
                                                  
           

              <div class="new_card_btns">

              
             <button name="cloud_package" value="{{cloud_lists.cl_type_id}}" class="{% if cloud_selec_list[0]==cloud_lists.cl_type_id %}add_to_carts{% else %}add_to_cart{%endif%}">Add To Cart</button>	

              </div>
            </div> 
          </div>                     
					
        </div>
			</div>

      {% endfor %}				



		</div>
  </form>

	</div>
</section>


<style>

    input {
        height: 11px;
        width: 17px;
        text-align: center;
        font-size: 26px;
        border: 1px solid #ddd;
        border-radius: 4px;
        display: inline-block;
        vertical-align: middle;
    }
    
    input[type=checkbox], input[type=radio] {
        margin: -5px 0 0;
        line-height: normal;
    }
    
    .new_card_pricebox { text-align:center}
    
    .new_card_box .new_card_btns button.add_to_carts {
        border-radius: 4px;
        background-color: green;
        color: #fff;
        border: 1px solid green;
        margin-bottom: 4px;
        font-family: Roboto, sans-serif;
        font-weight: 500;
        font-size: 12px;
        margin-top: 4px;
    }
    
    
</style>




<!-- footer Start -->
<footer class="footer section">
	<div class="container">
		
		
{% include 'footer.html' %}




  </body>
  </html>

  
<!--  Section Services End -->
